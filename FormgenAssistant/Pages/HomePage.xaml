<UserControl x:Class="FormgenAssistant.Pages.HomePage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FormgenAssistant.Pages"
             mc:Ignorable="d" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">

    <Grid Background="#FF0F0B1E" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
        
        <TextBlock TextWrapping="Wrap" Text="Dakota Jordan 2021" VerticalAlignment="Bottom" HorizontalAlignment="Right" Foreground="#FFD2D2D2" FontSize="16"/>
        <Button Width="75" Height="50" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" Margin="643,319,30,31" Style="{DynamicResource ButtonStyle1}" Click="Button_Click" VerticalAlignment="Bottom" HorizontalAlignment="Right">
            <Image HorizontalAlignment="Left" Height="45" VerticalAlignment="Top" Width="70" Source="/GitHub-logo-White.png" Stretch="Fill"/>
        </Button>

    </Grid>
</UserControl>
